<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFE0B2"
    tools:context="com.blackflag.roadsandhighwaysdepartment.fragment.EGPTender">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="75dp">
        <LinearLayout
            android:layout_width="match_parent"

            android:layout_height="wrap_content"

            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"
                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Dhaka"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_dhaka"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Chittagong"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/egp_chittagong"
                    android:layout_alignParentRight="true"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Barisal"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_barisal"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Khulna"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_khulna"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Rajshahi"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_rajshahi"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Shylet"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_shylet"
                    />

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Gopalgonj"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_gopalgonj"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Mymensingh"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_mymensingh"/>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="8dp"
                android:layout_margin="5dp"

                android:background="#FFCC80"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="30sp"
                    android:textColor="#000000"
                    android:paddingLeft="10dp"
                    android:text="Rangpur"/>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:id="@+id/egp_rangpur"/>

            </RelativeLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:elevation="15dp"
                android:layout_margin="5dp"

                android:background="#795548"
                android:padding="10dp"
                android:layout_height="60dp"
                >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:textSize="35sp"
                    android:textColor="#ffffff"

                    android:paddingLeft="10dp"
                    android:gravity="center"
                    android:id="@+id/save"
                    android:text="Save"/>


            </RelativeLayout>






        </LinearLayout>
    </ScrollView>

</FrameLayout>
